  "bugs": {
    "url": "https://github.com/bryantee/pirate-bartender/issues"
  },
  "homepage": "https://github.com/bryantee/pirate-bartender#readme",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "chokidar-cli": "^1.2.0",
    "less": "^2.7.1",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1"
  }
}
